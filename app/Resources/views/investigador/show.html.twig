{% extends 'layout.html.twig' %}

{% block body -%}
    <h1>Investigador</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.Id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ entity.UserName }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ entity.Password }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ entity.Email }}</td>
            </tr>
            <tr>
                <th>Isactive</th>
                <td>{{ entity.IsActive }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ entity.Name }}</td>
            </tr>
            <tr>
                <th>Activationtoken</th>
                <td>{{ entity.ActivationToken }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('investigador') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('investigador_edit', { 'id': entity.Id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
