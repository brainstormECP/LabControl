{% extends 'layout.html.twig' %}

{% block body -%}
    <h1>EntradaReactivo list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fecha</th>
                <th>Cantidad</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('entradareactivo_show', { 'id': entity.Id }) }}">{{ entity.Id }}</a></td>
                <td>{% if entity.Fecha %}{{ entity.Fecha|date('Y-m-d H:i:s') }}{% endif %}</td>
                <td>{{ entity.Cantidad }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('entradareactivo_show', { 'id': entity.Id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('entradareactivo_edit', { 'id': entity.Id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('entradareactivo_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
