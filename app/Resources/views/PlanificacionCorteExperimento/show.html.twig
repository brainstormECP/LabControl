{% extends 'layout.html.twig' %}

{% block content -%}
    <h1>Planificacion</h1>

    <table class="record_properties">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Fecha</th>
            <td>{{ entity.fecha|date('Y-m-d H:i:s') }}</td>
        </tr>
        <tr>
            <th>Experimento</th>
            <td>{{ entity.experimento }}</td>
        </tr>
        <tr>
            <th>Descripcion</th>
            <td>{{ entity.descripcion }}</td>
        </tr>
        </tbody>
    </table>
    <a class="btn btn-default"  href="{{ path('planificacioncorteexperimento') }}">
        Lista
    </a>
    {{ form(delete_form) }}

{% endblock %}
