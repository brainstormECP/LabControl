{% extends 'layout.html.twig' %}

{% block content -%}
    <h1>Reactivo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.Id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.Nombre }}</td>
            </tr>
        </tbody>
    </table>
    <div class="form-group">
        <a class="btn btn-default" href="{{ path('reactivo') }}">Lista de Reactivos</a>
        {{ form(delete_form) }}
    </div>
{% endblock %}
