{% extends 'layout.html.twig' %}

{% block body -%}
    <h1>SalidaReactivo</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.Id }}</td>
            </tr>
            <tr>
                <th>Fecha</th>
                <td>{{ entity.Fecha|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Cantidad</th>
                <td>{{ entity.Cantidad }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('salidareactivo') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('salidareactivo_edit', { 'id': entity.Id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
