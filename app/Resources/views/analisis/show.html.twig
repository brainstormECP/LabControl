{% extends 'layout.html.twig' %}

{% block content -%}
    <h1>Analisis</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombre</th>
                <td>{{ entity.nombre }}</td>
            </tr>
            <tr>
                <th>Descripción</th>
                <td>{{ entity.imageFile }}</td>
            </tr>
            <tr>
                <th>Formula</th>
                <td>{{ entity.formula }}</td>
            </tr>
        </tbody>
    </table>

    <div class="form-group">
        <a class="btn btn-default" href="{{ path('analisis') }}">Lista de Análisis</a>
        {{ form(delete_form) }}
    </div>
{% endblock %}
