{% extends 'layout.html.twig' %}

{% block content -%}
    <h1>Inventario de reactivo</h1>

    <table class="table">
        <tbody>
        <tr>
            <th>Id</th>
            <td>{{ entity.id }}</td>
        </tr>
        <tr>
            <th>Reactivo</th>
            <td>{{ entity.reactivo }}</td>
        </tr>
        <tr>
            <th>Existencia</th>
            <td>{{ entity.cantidad }}</td>
        </tr>
        </tbody>
    </table>
    <div class="form-group">
        <a class="btn btn-default" href="{{ path('institucion') }}">Lista de Instituciones</a>
        {{ form(delete_form) }}
    </div>
{% endblock %}
